{
  "homepage": "https://robd01.github.io/Full-Stack-Open/",
  "name": "Full Stack Open",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "axios": "^1.1.3",
    "bootstrap": "^5.2.2",
    "concurrently": "^7.5.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "server": "json-server -p3001 --watch db.json",
    "dev": "concurrently \"cd ../Full-Stack-Open-Backend && npm run server\" \"npm start\" ",
    "test": "concurrently \"cd ../Full-Stack-Open-Backend && npm run start:test\" \"npm start\" ",
    "eslint": "eslint .",
    "cypress:open": "cypress open",
    "test:e2e": "cypress run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "cypress": "^12.2.0",
    "eslint-plugin-jest": "^27.1.6",
    "gh-pages": "^4.0.0",
    "jest-watch-typeahead": "^0.6.5",
    "json-server": "^0.17.0",
    "react-router-dom": "^6.4.2"
  }
}
